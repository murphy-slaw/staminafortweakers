{
  "schemaVersion": 1,
  "id": "template-mod",
  "version": "${version}",
  "name": "Stamina For Tweakers",
  "description": "A painfully configurable stamina mod.",
  "authors": [
    "murphy slaw"
  ],
  "contact": {
    "homepage": "https://fabricmc.net/",
    "sources": "https://github.com/FabricMC/fabric-example-mod"
  },
  "license": "MIT",
  "icon": "assets/template-mod/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "net.funkpla.staminafortweakers.StaminaForTweakers"
    ],
    "client": [
      "net.funkpla.staminafortweakers.StaminaForTweakersClient"
    ]
  },
  "mixins": [
    "hamina-stamina.mixins.json",
    {
      "config": "hamina-stamina.client.mixins.json",
      "environment": "client"
    }
  ],
  "depends": {
    "fabricloader": ">=0.15.11",
    "minecraft": "~1.20.1",
    "java": ">=17",
    "fabric-api": "*"
  },
  "suggests": {
    "another-mod": "*"
  }
